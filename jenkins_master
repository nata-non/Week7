pipeline {
    agent any

    stages {
        
        stage('Welcome to master'){
            steps{
                echo 'Start Program'
            }
        }

        stage('Tigger the Slave Jobs') {
            steps {
            script {
                    def file = readFile "./nameages.txt"
                    def lines = file.readLines()
                    env.lines = lines
                }
                echo 'Start Trigger to send name and age to slave'
                build job: 'slave', parameters: [string(name: 'NAME', value: "$lines[0]"), string(name: 'Ages', value: "$lines[1]")]
            }
        }
    }
}
