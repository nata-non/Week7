pipeline {
    agent any
    parameters {
        string(name: 'NAME', defaultValue: 0i', description: 'Enter your name')
        string(name: 'Ages', defaultValue: '0', description: 'Enter your ages')
    }
    stages {
        stage('Example') {
            steps {
                echo "Hello from Slave ${params.NAME} of ${params.Ages}"
                
            }
        }
    }
}
